    "minimum-stability": "stable",
    "require": {
        "php": "^7.1",
        "ext-pcntl": "*",
        "php-ds/php-ds": "^1.2"
    },
    "suggest": {
        "ext-ds": "Native php-ds implementation"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "phpspec/phpspec": "^5.1"
    }
