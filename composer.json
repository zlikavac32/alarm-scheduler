{
    "name": "zlikavac32/alarm-scheduler",
    "description": "SIGALRM scheduler",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marijan Å uflaj",
            "email": "msufflaj32@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Zlikavac32\\AlarmScheduler\\": "src/"
        }
    },
    "autoload-dev": {
        "files": [
            "src-test/PHPUnit/factories.php",
            "src-test/PHPUnit/misc.php"
        ],
        "psr-4": {
            "Zlikavac32\\AlarmScheduler\\Tests\\": "tests/",
            "Zlikavac32\\AlarmScheduler\\TestHelper\\": "src-test/"
        }
